<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.kiadzaky.individual_task">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_demokrat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_demokrat_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Individual_task"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ui.instruktur.AddInstrukturActivity"
            android:exported="false" />
        <activity
            android:name=".ui.peserta.AddPesertaActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Individual_task.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>